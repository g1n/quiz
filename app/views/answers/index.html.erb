<h1>Answers</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Level</th>
    <th>Value</th>
	<th>Positive</th>
  </tr>
 
  <% @answers.each do |answer| %>
    <tr>
      <td><%= answer.id %></td>
      <td><%= answer.level %></td>
      <td><%= answer.value %></td>
	  <td><%= answer.positive %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New answer', new_answer_path %>